{
  "name": "@carloshatus/to_log",
  "version": "1.0.17",
  "description": "A small lib to organize logs at your service, compatible with AWS Lambda logging and AWS CloudWatch logs.",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest --config ./jest.config.js",
    "test:cov": "NODE_OPTIONS=--experimental-vm-modules npx jest --config ./jest.config.js --coverage",
    "test:dev": "NODE_OPTIONS=--experimental-vm-modules npx jest --config ./jest.config.js --coverage --watch",
    "prepublishOnly": "npx tsc",
    "build": "npx tsc"
  },
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carloshatus/toLog.git"
  },
  "author": "Carlos Hatus",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/carloshatus/toLog/issues"
  },
  "homepage": "https://github.com/carloshatus/toLog#readme",
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/node": "^22.10.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "debug": "^4.4.0"
  },
  "keywords": [
    "logging",
    "debug",
    "console",
    "AWS",
    "CloudWatch"
  ]
}